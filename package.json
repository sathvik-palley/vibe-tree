{
  "name": "vibetree-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "VibeTree - Vibe code with AI in parallel git worktrees",
  "scripts": {
    "dev": "turbo run dev",
    "dev:desktop": "pnpm --filter @vibetree/desktop dev",
    "dev:server": "pnpm --filter @vibetree/server dev",
    "dev:web": "pnpm --filter @vibetree/web dev",
    "dev:all": "pnpm build:deps && concurrently \"pnpm dev:server\" \"pnpm dev:web\"",
    "dev:core": "pnpm --filter @vibetree/core dev",
    "dev:ui": "pnpm --filter @vibetree/ui dev",
    "build": "turbo run build",
    "build:deps": "pnpm --filter @vibetree/core build && pnpm --filter @vibetree/ui build",
    "build:web": "pnpm build:deps && pnpm --filter @vibetree/web build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "package:desktop": "pnpm --filter @vibetree/desktop package",
    "deploy": "./docker-deploy.sh",
    "docker:build": "docker build -t vibetree-webapp .",
    "docker:run": "docker run -p 3000:3000 -p 3002:3002 --name vibetree-container vibetree-webapp",
    "docker:stop": "docker stop vibetree-container && docker rm vibetree-container",
    "docker:dev": "docker run -p 3000:3000 -p 3002:3002 --rm --name vibetree-dev vibetree-webapp",
    "docker:compose": "docker-compose up",
    "docker:compose:build": "docker-compose up --build",
    "docker:compose:down": "docker-compose down"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "git",
    "ai",
    "claude",
    "worktree",
    "terminal"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "turbo": "^1.11.2",
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}